<% layout('../layout.ejs') -%>
<section class="row">
	<article class="col-md-6">
		<ng-view></ng-view>	
		<div style="height:90px">
			<%- partial('../partials/slideshow.ejs') %>
		</div>	
    </article>
    <aside class="col-md-4">
		<div id="photoCarousel" class="carousel slide">
		    <ol class="carousel-indicators">
				<li data-target="#photoCarousel" data-slide-to="0" class="active"></li>
				<li data-target="#photoCarousel" data-slide-to="1"></li>
				<li data-target="#photoCarousel" data-slide-to="2"></li>
				<li data-target="#photoCarousel" data-slide-to="3"></li>
		    </ol>
			<div class="carousel-inner">
				<div class="item active"><img class="span6 img-polaroid" src="/images/slideshow1.jpg" alt="Power Plant"></div>
				<div class="item"><img class="span6 img-polaroid" src="/images/slideshow2.jpg" alt="Power Plant"></div>
				<div class="item"><img class="span6 img-polaroid" src="/images/slideshow3.jpg" alt="Power Plant"></div>
				<div class="item"><img class="span6 img-polaroid" src="/images/slideshow4.jpg" alt="Power Plant"></div>
			</div><!-- end carousel-inner -->
			<!-- Controls -->
			<a class="left carousel-control" href="#photoCarousel" data-slide="prev">
				<span class="icon-prev"></span>
			</a>
			<a class="right carousel-control" href="#photoCarousel" data-slide="next">
				<span class="icon-next"></span>
			</a>
        </div><!-- end artCarousel -->
    </aside>
    <div class="col-md-2"></div>
</section>